#sety145  #setcf000000      </GroupHeader>

                <DataSet1>
                    <Body> #tab025  $subfield1   #tab80 $subfield2 #tab180 $subfield3  #tab360 $subfield4  #tab455 $subfield5 #tab540 $subfield6  $subfield7 
 #tab020#lineH710</Body>

                    <Query>
                        <Select>ITEM.id_item,        ITEM.codigo,        ITEM.nombre,        UMB.nombre as desc_umb,      SUM(INGDET.cantidad),        INGDET.costo_unitario,        (SUM(INGDET.cantidad)*INGDET.costo_unitario) as costo_total</Select>
                        <From>almin.tal_ingreso ING INNER JOIN almin.tal_ingreso_detalle INGDET ON ING.id_ingreso=INGDET.id_ingreso INNER JOIN almin.tal_item ITEM ON INGDET.id_item=ITEM.id_item INNER JOIN param.tpm_unidad_medida_base UMB ON UMB.id_unidad_medida_base=ITEM.id_unidad_medida_base INNER JOIN almin.tal_almacen_logico ALMLOG ON ING.id_almacen_logico=ALMLOG.id_almacen_logico INNER JOIN almin.tal_almacen_ep ALMEP ON ALMEP.id_almacen_ep=ALMLOG.id_almacen_ep INNER JOIN almin.tal_almacen ALMA ON ALMEP.id_almacen=ALMA.id_almacen</From>
                        <Where>ING.fecha_finalizado_cancelado&gt;=$fecha_inicio and ING.fecha_finalizado_cancelado&lt;=$fecha_fin and ALMA.id_almacen=$var2 and ING.estado_ingreso='Finalizado'</Where>
                        <GroupBy>ITEM.id_item,ITEM.codigo,ITEM.nombre,UMB.nombre,INGDET.costo_unitario</GroupBy>
                        <OrderBy>ITEM.nombre</OrderBy>
                        <Config>
