    <DataSet>
        <DataSource>
            <Name>endesis_desarrollo</Name>
        </DataSource>

        <Query>
            <Select>DISTINCT ALMA.nombre, ALMA.id_almacen</Select>
            <From>almin.tal_almacen ALMA  INNER JOIN almin.tal_almacen_ep ALMEP ON ALMEP.id_almacen=ALMA.id_almacen INNER JOIN almin.tal_almacen_logico ALMLOG ON ALMLOG.id_almacen_ep=ALMEP.id_almacen_ep INNER JOIN almin.tal_ingreso ING ON ING.id_almacen_logico=ALMLOG.id_almacen_logico INNER JOIN almin.tal_ingreso_detalle INGDET ON INGDET.id_ingreso=ING.id_ingreso INNER JOIN almin.tal_item ITEM ON ITEM.id_item=INGDET.id_item</From>
            <Where>ALMA.id_almacen LIKE $id_almacen AND ING.estado_ingreso='Finalizado'</Where>
            <GroupBy></GroupBy>
            <OrderBy></OrderBy>
            <Config>
                <Distinct></Distinct>
                <OffSet>0</OffSet>
                <Limit>0</Limit>
            </Config>

        </Query>

        <Groups>
            <Config>
                <ShowGroup></ShowGroup>
                <ShowDetail>1</ShowDetail>
                <ShowLabel></ShowLabel>
