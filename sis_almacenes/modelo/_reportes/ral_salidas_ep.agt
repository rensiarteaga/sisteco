                    <Query>
                        <Select>distinct SALDET.id_salida, (select SUM(COALESCE (SALDET.costo,0))                                                 from almin.tal_salida_detalle SALDET                                                 WHERE SALDET.id_salida=SALIDA.id_salida)AS costo, MSALIDA.nombre, c.nro_cuenta, ALMLOG.nombre, SALIDA.fecha_finalizado_cancelado</Select>
                        <From>almin.tal_salida SALIDA INNER JOIN almin.tal_salida_detalle SALDET ON (SALDET.id_salida=SALIDA.id_salida) INNER JOIN almin.tal_almacen_logico ALMLOG ON (ALMLOG.id_almacen_logico=SALIDA.id_almacen_logico) INNER JOIN almin.tal_almacen_ep ALMAEP ON (ALMAEP.id_almacen_ep=ALMLOG.id_almacen_ep) INNER JOIN almin.tal_almacen ALMACE ON (ALMACE.id_almacen=ALMAEP.id_almacen) INNER JOIN almin.tal_motivo_salida_cuenta MSALCU ON (MSALCU.id_motivo_salida_cuenta=SALIDA.id_motivo_salida_cuenta)  INNER JOIN almin.tal_motivo_salida MSALIDA ON (MSALCU.id_motivo_salida= MSALIDA.id_motivo_salida)  INNER JOIN sci.tct_cuenta c ON (c.id_cuenta=MSALCU.id_cuenta)</From>
                        <Where>ALMACE.fecha_cierre &gt;= $fecha_cierre and ALMACE.fecha_apertura &lt;= $fecha_apertura and SALIDA.estado_salida='Finalizado' and ALMAEP.id_almacen_ep like $var1 and ALMACE.id_almacen like $var11</Where>
                        <GroupBy>SALIDA.id_salida, SALDET.id_salida, MSALIDA.nombre, c.nro_cuenta, ALMLOG.nombre, SALIDA.fecha_finalizado_cancelado</GroupBy>

                        <OrderBy></OrderBy>
