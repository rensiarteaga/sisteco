    <DataSet>
        <DataSource>
            <Name>endesis_factur23</Name>
        </DataSource>

        <Query>

            <Select>distinct ALMAEP.id_almacen_ep, ALMACE.fecha_cierre, ALMACE.fecha_apertura, fina.nombre_financiador, regi.nombre_regional, prog.nombre_programa, proy.nombre_proyecto, acti.nombre_actividad, ALMAEP.descripcion, ALMACE.nombre, ALMACE.id_almacen</Select>
            <From>almin.tal_salida SALIDA INNER JOIN almin.tal_almacen_logico ALMLOG ON (ALMLOG.id_almacen_logico=SALIDA.id_almacen_logico) INNER JOIN almin.tal_almacen_ep ALMAEP ON (ALMAEP.id_almacen_ep=ALMLOG.id_almacen_ep) INNER JOIN almin.tal_almacen ALMACE ON (ALMACE.id_almacen=ALMAEP.id_almacen) INNER JOIN almin.tal_motivo_salida_cuenta MSALCU ON (MSALCU.id_motivo_salida_cuenta=SALIDA.id_motivo_salida_cuenta)  INNER JOIN almin.tal_motivo_salida MSALIDA ON (MSALCU.id_motivo_salida= MSALIDA.id_motivo_salida)  INNER JOIN sci.tct_cuenta c ON (c.id_cuenta=MSALCU.id_cuenta) INNER JOIN param.tpm_fina_regi_prog_proy_acti    frppa ON (frppa.id_fina_regi_prog_proy_acti=ALMAEP.id_fina_regi_prog_proy_acti) INNER JOIN param.tpm_programa_proyecto_actividad ppa on (frppa.id_prog_proy_acti=ppa.id_prog_proy_acti) LEFT JOIN param.tpm_financiador fina ON frppa.id_financiador= fina.id_financiador LEFT JOIN param.tpm_regional regi ON frppa.id_regional= regi.id_regional LEFT JOIN param.tpm_programa prog  ON ppa.id_programa = prog.id_programa LEFT JOIN param.tpm_proyecto proy  ON ppa.id_proyecto= proy.id_proyecto LEFT JOIN param.tpm_actividad acti ON ppa.id_actividad = acti.id_actividad INNER JOIN almin.tal_salida_detalle SALDET ON (SALDET.id_salida=SALIDA.id_salida)</From>
            <Where>ALMACE.fecha_cierre &gt;= $fecha_cierre and ALMACE.fecha_apertura &lt;= $fecha_apertura AND SALIDA.estado_salida='Finalizado'  AND ALMACE.id_almacen like $id_almacen AND frppa.id_financiador like $id_financiador  and frppa.id_regional like $id_regional AND ppa.id_programa like $id_programa AND ppa.id_proyecto like $id_proyecto AND ppa.id_actividad like $id_actividad</Where>
            <GroupBy></GroupBy>
            <OrderBy></OrderBy>
            <Config>

                <Distinct></Distinct>
                <OffSet>0</OffSet>
                <Limit>0</Limit>
            </Config>

        </Query>

