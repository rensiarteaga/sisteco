    <Parameters>
        <id_regional>
            <value>%</value>
        </id_regional>

        <id_tipo_activo>
            <value>%</value>
        </id_tipo_activo>

        <id_sub_tipo_activo>
            <value>%</value>
        </id_sub_tipo_activo>

        <id_activo_fijo>
            <value>%</value>
        </id_activo_fijo>

        <id_financiador>
            <value>%</value>
        </id_financiador>

        <id_programa>
            <value>%</value>
        </id_programa>

        <id_proyecto>
            <value>%</value>
        </id_proyecto>

    </Parameters>

    <DataSet>
        <DataSource>
            <Name>activos</Name>
        </DataSource>

        <Query>
            <Select>DISTINCT a.codigo,        a.id_activo_fijo,        a.descripcion,        tipo.descripcion as desc_tipo_activo,        subtip.descripcion as desc_sub_tipo_activo,        fin.nombre_financiador,        regi.nombre_regional,        prog.nombre_programa,        proy.nombre_proyecto</Select>
            <From>actif.taf_activo_fijo a      INNER JOIN actif.taf_activo_fijo_tpm_frppa afrppa ON a.id_activo_fijo =       afrppa.id_activo_fijo      INNER JOIN param.tpm_fina_regi_prog_proy_acti frppa ON       afrppa.id_fina_regi_prog_proy_acti = frppa.id_fina_regi_prog_proy_acti      INNER JOIN param.tpm_financiador fin ON frppa.id_financiador = fin.id_financiador      INNER JOIN param.tpm_regional regi ON frppa.id_regional = regi.id_regional      INNER JOIN param.tpm_programa_proyecto_actividad ppa ON ppa.id_prog_proy_acti =       frppa.id_prog_proy_acti      INNER JOIN param.tpm_programa prog ON prog.id_programa = ppa.id_programa      INNER JOIN param.tpm_proyecto proy ON proy.id_proyecto = ppa.id_proyecto      INNER JOIN actif.taf_sub_tipo_activo subtip ON a.id_sub_tipo_activo =       subtip.id_sub_tipo_activo      INNER JOIN actif.taf_tipo_activo tipo ON subtip.id_tipo_activo =       tipo.id_tipo_activo</From>
            <Where>a.estado != 'eliminado' and       frppa.id_financiador LIKE $id_financiador AND        frppa.id_regional LIKE $id_regional AND       prog.id_programa LIKE $id_programa AND       proy.id_proyecto LIKE $id_proyecto AND       tipo.id_tipo_activo LIKE $id_tipo_activo AND       subtip.id_sub_tipo_activo LIKE $id_sub_tipo_activo AND       a.id_activo_fijo LIKE $id_activo_fijo</Where>
            <GroupBy></GroupBy>
            <OrderBy></OrderBy>
            <Config>
                <Distinct></Distinct>
                <OffSet>0</OffSet>
                <Limit>0</Limit>
            </Config>

        </Query>

