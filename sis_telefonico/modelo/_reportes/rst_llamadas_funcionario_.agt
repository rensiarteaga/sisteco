    <DataSet>
        <DataSource>
            <Name>fercho</Name>
        </DataSource>

        <Query>
            <Select>distinct empext.id_empleado, (COALESCE(PERSON.apellido_paterno,' ')||'  '||COALESCE(PERSON.apellido_materno,' ')||'  '||COALESCE(PERSON.nombre,' ')) AS desc_empleado,        geren.nombre_gerencia, sum(llamad.duracion_llamada) as suma</Select>
            <From>tst_llamada llamad        inner join tkp_empleado emplea        on llamad.id_empleado=emplea.id_empleado        inner join tsg_persona PERSON        on emplea.id_persona=PERSON.id_persona        inner join tkp_empleado_extension empext        on emplea.id_empleado=empext.id_empleado        inner join tst_gerencia geren        on empext.id_gerencia=geren.id_gerencia</From>
            <Where>empext.id_empleado=$empleado and geren.id_gerencia=$gerencia and llamad.fecha_llamada&gt;=$fecha_inicio and llamad.fecha_llamada&lt;=$fecha_fin</Where>
            <GroupBy>empext.id_empleado, PERSON.apellido_paterno, PERSON.apellido_materno, PERSON.nombre, geren.nombre_gerencia</GroupBy>
            <OrderBy></OrderBy>
            <Config>
                <Distinct></Distinct>
                <OffSet>0</OffSet>
                <Limit>0</Limit>
            </Config>

        </Query>

