                        <From>tst_llamada llamad        inner join tst_tipo_llamada tiplla        on llamad.id_tipo_llamada=tiplla.id_tipo_llamada        inner join tkp_empleado emplea        on llamad.id_empleado=emplea.id_empleado        inner join tsg_persona PERSON        on emplea.id_persona=PERSON.id_persona        inner join tkp_empleado_extension empext        on emplea.id_empleado=empext.id_empleado        inner join tst_gerencia geren        on empext.id_gerencia=geren.id_gerencia  left join tst_llamada llamad1 on llamad.id_llamada=llamad1.fk_id_llamada</From>
